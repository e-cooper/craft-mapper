ry of Folsom.","website":"http:\/\/www.lockdownbrewingcompany.com\/","isOrganic":"N","images":{"icon":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/anUUVa\/upload_JL1le5-icon.png","medium":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/anUUVa\/upload_JL1le5-medium.png","large":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/anUUVa\/upload_JL1le5-large.png"},"status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:00","updateDate":"2013-06-23 12:36:41","id":"195Jl2","name":"Rancho Cordova Production\/Tasting Room","streetAddress":"11327 Trade Center Dr. #350","locality":"Rancho Cordova","region":"California","postalCode":"95742","phone":"916-835-7416","website":"http:\/\/www.lockdownbrewingcompany.com\/","hoursOfOperation":"Wednesday-Friday 4-8pm","latitude":38.607202,"longitude":-121.263761,"isPrimary":"Y","inPlanning":"N","isClosed":"N","openToPublic":"Y","locationType":"tasting","locationTypeDisplay":"Tasting Room","countryIsoCode":"US","status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:00","updateDate":"2014-07-23 19:11:34","displayName":"United States","isoThree":"USA","createDate":"2012-01-03 02:41:33"},{"id":"eJlHAD","name":"Folsom Tasting Room","streetAddress":"718 Sutter St.","extendedAddress":"Suite 200","locality":"Folsom","region":"California","postalCode":"95630","phone":"(916) 358-9645","website":"http:\/\/www.lockdownbrewingcompany.com\/","hoursOfOperation":"Mon - Tue 11-3pm\r\nWed - Thu 11-10pm\r\nFri - Sat 11-12am\r\nSun 11-6pm","latitude":38.6774675,"longitude":-121.1771592,"isPrimary":"N","inPlanning":"N","isClosed":"N","openToPublic":"Y","locationType":"tasting","locationTypeDisplay":"Tasting Room","countryIsoCode":"US","status":"verified","statusDisplay":"Verified","createDate":"2013-06-23 12:38:40","updateDate":"2014-07-23 19:11:34","displayName":"United States","isoThree":"USA","createDate":"2012-01-03 02:41:33"},{"id":"GdU5Je","name":"LBC-HQ","streetAddress":"317 Leidesdorf St.","l