,{"id":"Q9BWPS","name":"Trumer Pils","description":"American and European brewmasters have come together to create the Trumer Brauerei Berkeley. With a passionate commitment to the Pilsner style, this international team has worked tirelessly to preserve every detail of the original Trumer Pils process of its centuries old brewery in Salzburg, Austria. Their efforts have paid off. Trumer Brauerei Berkeley combines the heritage and tradition of the old world with the new world's dedication to craft brewing.","website":"http:\/\/www.trumer-international.com\/","established":"2004","isOrganic":"N","images":{"icon":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/Q9BWPS\/upload_5UUtOe-icon.png","medium":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/Q9BWPS\/upload_5UUtOe-medium.png","large":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/Q9BWPS\/upload_5UUtOe-large.png"},"status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:11","updateDate":"2012-10-01 00:46:34","id":"33shrE","streetAddress":"1404 4th Street","locality":"Berkeley","region":"California","postalCode":"94710","phone":"510- 526-1160","website":"http:\/\/www.trumer-international.com\/","latitude":37.876741,"longitude":-122.303439,"isPrimary":"Y","inPlanning":"N","isClosed":"N","openToPublic":"Y","locationType":"micro","locationTypeDisplay":"Micro Brewery","countryIsoCode":"US","yearOpened":"2004","status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:11","updateDate":"2014-07-23 19:11:34","displayName":"United States","isoThree":"USA","createDate":"2012-01-03 02:41:33"},{"id":"mI2ZZh","name":"TRVE Brewing Company","description":"TRVE Brewing is the Denver brewery dedicated to bringing you beer that's beyond the pale. A neighborhood spot where you can hang out and have some killer beer.","website":"http:\/\/trvebrewing.com\/","established":"2012","isOrganic":"N","images":{"icon":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/mI2ZZh\/upload_bs6cjY-icon.png","medium":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/mI2ZZh\/upload_bs6cjY-medium.png","large":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/mI2ZZh\/upload_bs6cjY-large.png"},"status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:11","updateDate":"2012-09-29 22:10:01","id":"fNbW0f","streetAddress":"227 Broadway #101","locality":"Denver","region":"Colorado","postalCode":"80218","phone":"303-351-1021","website":"http:\/\/trvebrewing.com\/","hoursOfOperation":"Monday-Tuesday:Closed\r\nWednesday-Thursday:4PM-10PM\r\nFriday-Saturday:4PM-12AM\r\nSunday:2PM-8PM","latitude":39.719849,"longitude":-104.987507,"isPrimary":"Y","inPlanning":"N","isClosed":"N","openToPublic":"Y","locationType":"micro","locationTypeDisplay":"Micro Brewery","countryIsoCode":"US","yearOpened":"2012","status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:11","updateDate":"2014-07-23 19:11:34","displayName":"United States","isoThree":"USA","createDate":"2012-01-03 02:41:33"},{"id":"e2agAL","name":"Tsingtao","website":"http:\/\/tsingtaobeer.com\/","isOrganic":"N","images":{"icon":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/e2agAL\/upload_aoe3nf-icon.png","medium":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/e2agAL\/upload_aoe3nf-medium.png","large":"https:\/\/s3.amazonaws.com\/brewerydbapi\/brewery\/e2agAL\/upload_aoe3nf-large.png"},"status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:11","updateDate":"2013-05-10 02:00:23","id":"HhKKA6","locality":"Qingdao","region":"Shandong","website":"http:\/\/tsingtaobeer.com\/","latitude":36.087509,"longitude":120.34272,"isPrimary":"Y","inPlanning":"N","isClosed":"N","openToPublic":"Y","locationType":"micro","locationTypeDisplay":"Micro Brewery","countryIsoCode":"CN","status":"verified","statusDisplay":"Verified","createDate":"2012-01-03 02:42:11","updateDate":"2014-07-23 19:11:34","displayName":"China","isoThree":"CHN","numberCode":156,"createDate":"2012-